<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="solo">

    <!-- Quad model -->
    <include>
      <uri>model://solo_base</uri>
      <pose>0 0 0 0 0 0</pose>
    </include>

    <!-- DeLeaves -->
    <include>
      <uri>model://deleaves_clamp</uri>
      <pose>0 0 0 0 0 0</pose>
    </include>

    <!-- Attach DeLeaves to quad -->
    <joint name="deleaves_joint" type="universal">
      <parent>solo_base::base_link</parent>
      <child>deleaves_clamp::link</child>
      <pose>0 0 -0.1 0 0 0</pose>
      <axis>
        <xyz>1 0 0</xyz>
      </axis>
      <axis2>
        <xyz>0 1 0</xyz>
      </axis2>
    </joint>
    <!--
    -->

  </model>
</sdf>
